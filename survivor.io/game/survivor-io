        var bindEventListeners = function() {
            var elements = [].slice.call(document.getElementsByClassName(config.elementSelector));
            elements.forEach(function(element) {
                element.onclick = function(e) {
                    if (element.dataset.hasOwnProperty("target")) {
                        var target = element.dataset.target;
                        var targetBlank = !!(element.target === "_blank");
                        if (target !== undefined) {
                            if (targetBlank) {
                                window.open(atob(target), "_blank");
                            } else {
                                window.location.href = atob(target);
                            }
                        }
                    }
                }
                ;
            });
        };

        bindEventListeners();

    }
    )();
</script>
<!-- PAGE OK GAME -->
