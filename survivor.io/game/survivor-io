<script>
    window.addEventListener("load", function() {
        setTimeout(function() {
            var head = document.getElementsByTagName('head')[0];
            var link = document.createElement('link');
            link.id = "miniplay-styles";
            link.rel = 'stylesheet';
            link.type = 'text/css';
            link.href = 'https://s2.minijuegosgratis.com/mobile/styles/css/lightweight-minicons_1765344294.css?v=_1765344294';
            link.media = 'all';
            head.appendChild(link);
        }, 500);
        setTimeout(function() {
            var element;
            setTimeout(function() {
                element = document.getElementById("game-tags-cloud");
                if (element)
                    element.children[0].children[0].click();
            }, 260);
            setTimeout(function() {
                element = document.getElementById("footer-accordion");
                if (element)
                    element.children[0].children[0].click();
            }, 510);
        }, 1000);

        setTimeout(function() {
            var b64_atob = function(str) {
                return decodeURIComponent(escape(window.atob(str)));
            };
            setTimeout(function() {
                element = document.getElementById("game-latest-games");
                if (element && element.getAttribute("data-content")) {
                    element.innerHTML = b64_atob(element.getAttribute("data-content"));
                    element.setAttribute("data-content", "");
                }
            }, 275);
            setTimeout(function() {
                element = document.getElementById("minijuegos-app-showcase");
                if (element && element.getAttribute("data-content")) {
                    element.innerHTML = b64_atob(element.getAttribute("data-content"));
                    element.setAttribute("data-content", "");
                }
            }, 525);
            setTimeout(function() {
                element = document.getElementById("game-comments");
                if (element && element.getAttribute("data-content")) {
                    element.innerHTML = b64_atob(element.getAttribute("data-content"));
                    element.setAttribute("data-content", "");
                }
            }, 775);
        }, 2500);
    });
</script>
