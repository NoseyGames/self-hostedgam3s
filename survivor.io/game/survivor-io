<script>
    // Create a namespace for Minipush initialization
    window.MinipushManager = {
        settings: {
            client: {
                id: "d.6abad.3ce1bd0",
                apiKey: "feab899abcab6ff2138291f5f4408d79"
            },
            debug: false,
            autoSubscribe: false,
            serviceWorker: {
                url: "https://www.miniplay.com/sw.js"
            }
        },

        init: function() {
            if (!window.Minipush) {
                window.Minipush = [];
            }

            Minipush.push(["settings", this.settings]);
            Minipush.push(["setId", ""]);
            Minipush.push(["setTags", ["web-m_miniplay_com", "user-guest"]]);
            Minipush.push(function() {
                if (!Minipush.isSubscribed() && !Minipush.isBlocked()) {
                    Minipush.subscription.subscribe();
                }
            });
        },

        loadScript: function() {
            const script = document.createElement('script');
            script.src = 'https://pushtide.com/minipush-client/minipush/minipush-latest.js';
            script.async = true;
            script.onload = () => this.init();
            document.head.appendChild(script);
        }
    };

    // Wait for window load and then delay further to ensure everything else is loaded
    window.onload = function() {
        // Move to the end of the event queue and delay by 5 seconds
        setTimeout( () => {
            // Add another setTimeout to ensure we're really at the end
            setTimeout( () => MinipushManager.loadScript(), 0);
        }
        , 5000);
    }
    ;
</script>
<script type="application/javascript">
    if ('serviceWorker'in navigator) {
        window.addEventListener('load', function() {
            // navigator.serviceWorker.register(App.url.root + 'assets/js/App/SW/mpsw.js')
            navigator.serviceWorker.register('https://www.miniplay.com/mpsw.js').then(registration => {//console.log('Service Worker is registered', registration);
            }
            ).catch(err => {//console.error('Registration failed:', err);
            }
            );
        });
    }
</script>
